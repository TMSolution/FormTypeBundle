                    {%- block duallist_widget -%}

                    {{ block('choice_widget') }} 
                    {% set notSelectedList=[] %}
                    {% set selectedList=[] %}

                    {% for group_label, choice in choices %}
                        {% if choice is selectedchoice(value)%} 

                            {% set selectedList = selectedList|merge(['<tr class="duallist-record selected" data-choiceid="'~choice.value~'">
                                <td class="user-horizontalist-img-container">
                                    <img class="img-circle user-profile-thumbnail" alt="" src="'~userservice.getAvatarImage(choice.data)~'"> 
                                </td>
                                <td>
                                    <div class="d-block">
                                        <span class="f-400 f-15 c-black">'~choice.label|trans~'</small> 
                                    </div>

                                </td>
                                <td style="width:30px">
                                    <ul style="width:30px" class="actions f-18 t-a-r di-block ">
                                        <li class="actions di-block">
                                            <a class="cr-pointer cancel-netmeetinginvitation-btn">
                                                <i class="zmdi zmdi-close"></i>
                                            </a>
                                        </li>
                                    </ul>
                                </td>
                            </tr>']) %}



                        {%else%} 
                            {% set notSelectedList = notSelectedList|merge(['<tr class="duallist-record not-selected" data-choiceid="'~choice.value~'">
                                <td class="user-horizontalist-img-container">
                                    <img class="img-circle user-profile-thumbnail" alt="" src="'~userservice.getAvatarImage(choice.data)~'"> 
                                </td>
                                <td>
                                    <div class="d-block">
                                        <span class="f-400 f-15 c-black">'~choice.label|trans~'</small> 
                                    </div>

                                </td>
                                
                            </tr>']) %}

                        {% endif %}
                    {% endfor %}

                    <div class="duallist m-b-30">   



                        <div class="card-body">
                            <div class="row  vertical-align">
                                <div class="col-md-5">
                                    <div class="table-responsive ">
                                        <table class="table table-striped table-hover table-full-width bootgrid-table fix-table horizontalist-table">
                                            <thead>
                                                <tr>
                                                    <th colspan="2">{{'system.duallist.not_selected'|trans}}</th>
                                                </tr>
                                            </thead>
                                            <tbody class="duallist-not-selected-container">
                                                {{ notSelectedList|join|raw }}
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <div class="col-md-2 vertical-col" style="text-align:center">
                                    <button class="btn btn-primary duallist-add-btn">
                                        <i class="zmdi zmdi-arrow-right"></i>
                                    </button>

                                </div>
                                <div class="col-md-5">
                                    <div class="table-responsive ">
                                        <table class="table table-striped table-hover table-full-width bootgrid-table fix-table horizontalist-table">
                                            <thead>
                                                <tr>
                                                    <th colspan="3">{{'system.duallist.selected'|trans}}</th>
                                                </tr>
                                            </thead>
                                            <tbody class="duallist-selected-container">
                                                {{ selectedList|join|raw }}
                                            </tbody>
                                        </table>
                                    </div>
                                </div>

                            </div>

                        </div>

                    </div>


                    {% if(lazyjs) %} {{ jsblocklater(block('duallist_widget_script')) }}{% endif%}
                    {%- endblock duallist_widget -%}





                    {%- block duallist_widget_script -%}
                    <script>
                                $(document).off("click", '.duallist-not-selected-container .duallist-record');
                                $(document).on('click', '.duallist-not-selected-container .duallist-record', function(){

                        if ($(this).hasClass('active')){
                        $(this).removeClass('active');
                        }
                        else{
                        $(this).addClass('active');
                        }

                        if ($('.duallist-not-selected-container').find('.duallist-record.not-selected.active').length > 0){
                        $('.duallist-add-btn').attr('disabled', null)
                        }
                        else{
                        $('.duallist-add-btn').attr('disabled', 1)
                        }

                        });
                                $(document).off("click", '.duallist-add-btn');
                                $(document).on('click', '.duallist-add-btn', function(e){
                        e.preventDefault();
                                $('.duallist-not-selected-container').find('.duallist-record.not-selected.active').each(function(){

                        $(this).clone().removeClass('not-selected').removeClass('active').addClass('selected').append('<td><ul style="width:30px" class="actions f-18 t-a-r di-block "><li class="actions di-block"><a class="cr-pointer cancel-netmeetinginvitation-btn"><i class="zmdi zmdi-close"></i></a></li></ul></td>').appendTo('.duallist-selected-container');
                                $("#netmeeting_users option[value='" + $(this).attr('data-choiceid') + "']").prop("selected", true);
                                $(this).remove();
                        });
                        });
                                $(document).off("click", '.cancel-netmeetinginvitation-btn');
                                $(document).on('click', '.cancel-netmeetinginvitation-btn', function(e){




                        e.preventDefault();
                                var el = $(this).closest('tr').clone();
                                el.find("td:last-child").remove();
                                el.removeClass('selected').addClass('not-selected').appendTo('.duallist-not-selected-container');
                                $("#netmeeting_users option[value='" + $(this).closest('tr').attr('data-choiceid') + "']").prop("selected", false);
                                $(this).closest('tr').remove();
                        });






                    </script>
                    {%- endblock duallist_widget_script -%}
                    
                    
                    
                    
                    
                    
                    
                    
                    
  
                    
